<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<message-bundle>labels</message-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>actAtualizaCliente</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemcliente.ActAtualizaCliente</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlCliente</property-name>
			<value>#{clienteCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actLogin</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.login.ActLogin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlUsuario</property-name>
			<value>#{usuarioCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actConsultaCliente</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemcliente.ActConsultaCliente</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlCliente</property-name>
			<value>#{clienteCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actAtualizaProduto</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemproduto.ActAtualizaProduto</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlProduto</property-name>
			<value>#{produtoCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actConsultaProduto</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemproduto.ActConsultaProduto</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlProduto</property-name>
			<value>#{produtoCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actAtualizaFornecedor</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemfornecedor.ActAtualizaFornecedor</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlFornecedor</property-name>
			<value>#{fornecedorCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actConsultaFornecedor</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemfornecedor.ActConsultaFornecedor</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlFornecedor</property-name>
			<value>#{fornecedorCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actVenda</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.venda.ActVenda</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlVenda</property-name>
			<value>#{vendaCtl}</value>
		</managed-property>
		<managed-property>
			<property-name>ctlCliente</property-name>
			<value>#{clienteCtl}</value>
		</managed-property>
		<managed-property>
			<property-name>ctlCompra</property-name>
			<value>#{compraCtl}</value>
		</managed-property>
		<managed-property>
			<property-name>ctlProduto</property-name>
			<value>#{produtoCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actCompra</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.compra.ActCompra</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlCompra</property-name>
			<value>#{compraCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actPrincipal</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.principal.ActPrincipal</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlPrincipal</property-name>
			<value>#{principalCtl}</value>
		</managed-property>
		<managed-property>
			<property-name>ctlUsuario</property-name>
			<value>#{usuarioCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actAtualizaUsuario</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemusuario.ActAtualizaUsuario</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlUsuario</property-name>
			<value>#{usuarioCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actConsultaUsuario</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.mantemusuario.ActConsultaUsuario</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>ctlUsuario</property-name>
			<value>#{usuarioCtl}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actRelatorio</managed-bean-name>
		<managed-bean-class>br.absolut.apresentacao.relatorio.ActRelatorio</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>ctlVenda</property-name>
			<value>#{vendaCtl}</value>
		</managed-property>
		<managed-property>
			<property-name>ctlCompra</property-name>
			<value>#{compraCtl}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/web/apresentacao/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>atualizaCliente</from-outcome>
			<to-view-id>/web/apresentacao/cliente/atualizacliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>atualizaFornecedor</from-outcome>
			<to-view-id>/web/apresentacao/fornecedor/atualizafornecedor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>atualizaProduto</from-outcome>
			<to-view-id>/web/apresentacao/produto/atualizaproduto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>consultaCliente</from-outcome>
			<to-view-id>/web/apresentacao/cliente/consultacliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>compra</from-outcome>
			<to-view-id>/web/apresentacao/compra/compra.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>consultaFornecedor</from-outcome>
			<to-view-id>/web/apresentacao/fornecedor/consultafornecedor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/web/apresentacao/principal/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>consultaProduto</from-outcome>
			<to-view-id>/web/apresentacao/produto/consultaproduto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>venda</from-outcome>
			<to-view-id>/web/apresentacao/venda/venda.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>atualizaUsuario</from-outcome>
			<to-view-id>/web/apresentacao/usuario/atualizausuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Geral</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>alteraSenha</from-outcome>
			<to-view-id>/web/apresentacao/usuario/alterasenha.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>